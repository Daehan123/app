password = input("비밀번호를 입력하세요 : ")

letters = "abcdefghijklmnopqrstuvwxyz"
attempts = 0
found = False

for c1 in letters:
    for c2 in letters:
        for c3 in letters:
            for n in range(100):
                if n < 10:
                    num = "0" + str(n)
                else:
                    num = str(n)

                guess = c1 + c2 + c3 + num
                attempts += 1

                if attempts % 1000 == 0:
                    print(attempts, "번째 시도 중 현재:", guess)

                if guess == password:
                    print("비밀번호를 찾았습니다!")
                    print("비밀번호:", guess)
                    print("시도 횟수:", attempts)
                    found = True
                    break

            if found:
                break
        if found:
            break
    if found:
        break

if not found:
    print("비밀번호를 찾지 못했습니다.")
